#include<bits/stdc++.h>
using namespace std;


void solve(){
    int n;
    cin>>n;
    if(n==1 || n==3)
    cout<<"-1"<<endl;
    else if (n==2)
    cout<<"66"<<endl;
    else if(n==4)
    cout<<"3366"<<endl;
    
    else{
        if(n%2!=0){
            for(int j=0;j<n-4;j++)
            cout<<"3";
            cout<<"6366"<<endl;
        }

        else{
            for(int j=0;j<n-2;j++)
            cout<<"3";
            cout<<"66"<<endl;
        }
    }

}

int main(){
    int t;
    cin>>t;
    while(t--){
        solve();
    }
}
