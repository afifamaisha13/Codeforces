#include<bits/stdc++.h>
using namespace std;
int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);

    int n,m;
    cin>>n>>m;
    map<string,string>a;
    for(int i=0;i<n;i++){
        string s,t;
        cin>>s>>t;
        t.push_back(';');
        a[t]=s;
}

    for(int i=0;i<m;i++){
        string c,d;
        cin>>c>>d;
        cout<<c<<" "<<d<<" #"<<a[d]<<endl;
    }


    }
