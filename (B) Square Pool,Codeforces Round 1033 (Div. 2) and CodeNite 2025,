#include<bits/stdc++.h>
using namespace std;
#define ll long long
void solve(){
   int n,s,res=0;
   cin>>n>>s;
   for(int i=0;i<n;i++){
     int dx,dy,x,y;
     cin>>dx>>dy>>x>>y;
     if(dx+dy==0 && x+y==s)
     res++;
     if(dx==dy && x==y)
     res++;
     
   }
   
   cout<<res<<endl;
    }
    
    
int main(){
    int t;
    cin>>t;
    while(t--){
        solve();
    }
}
