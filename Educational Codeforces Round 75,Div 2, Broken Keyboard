#include <bits/stdc++.h>
using namespace std;

void solve(){

    string s;
    cin>>s;
    set<char>ans;
    for(int i=0;i<s.size();i++){
        if(s[i]==s[i+1])
            i++;
        else
            ans.insert(s[i]);

    }

    for(auto a:ans)
    cout<<a;
    cout<<endl;
    ans.clear();
}
int main(){
    int t;
    cin>>t;
    while(t--){
        solve();
    }
}


