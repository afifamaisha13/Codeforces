#include<bits/stdc++.h>
using namespace std;
int main(){
    ios_base::sync_with_stdio(0);
    cin.tie(0);

    int n,m;
    cin>>n;
    unordered_map<int,int>a;
    vector<int>v(n);
    for(int i=0;i<n;i++){
        cin>>v[i];
        a[v[i]]++;
}


int ans=0;
for(int i=0;i<n;i++){
ans = max ( ans, a[v[i]] );
}
cout<<ans<<endl;

    


    }
